<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Ingrediente" table="ingrediente">
		<id column="idIngrediente" name="idIngrediente">
			<generator class="identity"/>
		</id>
		<property column="nombre" name="nombre" type="string"/>
		<property column="sector" name="sector" type="string"/>
		<property column="costo" name="costo" type="float"/>
		<property column="unidad" name="unidad" type="String"/>
		<property column="existencia" name="existencia" type="boolean"/>
	</class>
</hibernate-mapping>